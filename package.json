{
  "author": "Ashish-simpleCoder",
  "private": false,
  "name": "fff-final",
  "version": "1.5.1",
  "description": "Testing the library deployement",
  "homepage": "https://github.com/Ashish-simpleCoder/fff-final",
  "keywords": [
    "awesome"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "pnpm run build",
    "release": "changeset publish",
    "build": "tsup && pnpm run post-build",
    "post-build": "node scripts/addTypes.js",
    "lint": "tsc",
    "test": "vitest --coverage",
    "coverage": "vitest run --coverage",
    "cs": "pnpm changeset"
  },
  "license": "MIT",
  "typesVersions": {
    "*": {
      ".": ["dist/index.d.ts"],
      "hooks": [
        "dist/hooks/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/esm/hooks/index.js",
      "default": "./dist/esm/hooks/index.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-istanbul": "^0.30.1",
    "jsdom": "^21.1.1",
    "react-dom": "^18.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
